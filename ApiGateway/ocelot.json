      "UpstreamHttpMethod": ["Get", "Put", "Delete"],

      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5002
        }
      ],
      "DownstreamPathTemplate": "/api/users/{userId}/posts/{postId}"
    },
    {
      "UpstreamPathTemplate": "/api/users",
      "UpstreamHttpMethod": ["Get", "Post"],

